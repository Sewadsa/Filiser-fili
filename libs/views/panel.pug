												li.lineInner(data-ref=""+link._id data-code=""+epi.series_id._id data-code2=""+epi._id)
													a.user(href="/user/"+link.user.username target="_blank" class="greenLink" title="Dodał:") !{link.user.username}

													- for(var i = 0, len = VIDEO_HOSTING_LIST.length; i < len; i++){
													- 	if(VIDEO_HOSTING_LIST[i].name==link.hosting){
													- 		embed = VIDEO_HOSTING_LIST[i].embed_code;
													- 		embed = embed.replace('#WIDTH', 1280);
													- 		embed = embed.replace('#HEIGHT', 720);
													- 		embed = embed.replace('#ID', link.video_id);
													- 		break;
													- 	}
													- }
													- if(link.hosting=='verystream') embed = embed.replace('/e/', '/stream/');
													- else if(link.hosting=='openload') embed = embed.replace('embed', 'f');
													- else if(link.hosting=='vidoza') embed = embed.replace('embed-', '');
													- else if(link.hosting=='gounlimited') embed = embed.replace('embed-', '');
													- else if(link.hosting=='premium') embed = embed.replace('zgdat01', 'zgdat02');

													a.hosting(href=embed target="_blank" class="greenLink") #{link.hosting}
													.videoId #{link.file_size ? BYTES_FORMAT(link.file_size, {unitSeparator: ' ', decimalPlaces: 2}):'Size: nie ustalono'}
													select.select(name="type" class="typeSelect")
														each val in VIDEO_TYPES_LIST
															option(value=""+val[0] selected=(link.type==val[0])) #{val[1]}
													select.select(name="quality" style="margin-left: 20px; margin-right: 20px;" class="qualitySelect")
														each val in VIDEO_QUALITY_LIST
															option(value=""+val[0] selected=(link.quality==val[0])) #{val[1]}
													i.fa(class="fa-times deleteLinkBtn" title="Usuń")
													i.fa(class="fa-check-square-o acceptLinkeBtn" title="Akceptuj")
													.clear

													if link.report!=''
														.zgloszenie Zgłoszenie:
														.report #{REPORT_TYPES_LIST[link.report]}
														.report_desc #{link.report_desc}
														.clear
								each movie, index in movie_links
									li.line(style=(index==0 && series_index>0?'margin-top: 100px;':''))
										a(href="/film/"+movie.url+"/"+movie.num target="_blank")
											img.poster(src=IMG_HOSTNAME+movie.poster width="24" height="24")
										.titles_box
											.title !{movie.title} (!{movie.year})
											.title_org !{movie.title_org}
										.btn_box
											a(href="http://www.filmweb.pl/film/"+movie.url+'-'+movie.filmweb_id target="_blank")
												i.fa(class="fa-external-link" title="Zobacz film w filmweb.pl")
										.clear
										ul
											each link in movie.links
												li.lineInner(data-ref=""+link._id data-code=""+movie._id)
													a.user(href="/user/"+link.user.username target="_blank" class="greenLink" title="Dodał:") !{link.user.username}

													- for(var i = 0, len = VIDEO_HOSTING_LIST.length; i < len; i++){
													- 	if(VIDEO_HOSTING_LIST[i].name==link.hosting){
													- 		embed = VIDEO_HOSTING_LIST[i].embed_code;
													- 		embed = embed.replace('#WIDTH', 1280);
													- 		embed = embed.replace('#HEIGHT', 720);
													- 		embed = embed.replace('#ID', link.video_id);
													- 		break;
													- 	}
													- }
													- if(link.hosting=='verystream') embed = embed.replace('/e/', '/stream/');
													- else if(link.hosting=='openload') embed = embed.replace('embed', 'f');
													- else if(link.hosting=='vidoza') embed = embed.replace('embed-', '');
													- else if(link.hosting=='gounlimited') embed = embed.replace('embed-', '');
													- else if(link.hosting=='premium') embed = embed.replace('zgdat01', 'zgdat02');

													a.hosting(href=embed target="_blank" class="greenLink") #{link.hosting}
													.videoId #{link.file_size ? BYTES_FORMAT(link.file_size, {unitSeparator: ' ', decimalPlaces: 2}):'Size: nie ustalono'}
													select.select(name="type" class="typeSelect")
														each val in VIDEO_TYPES_LIST
															option(value=""+val[0] selected=(link.type==val[0])) #{val[1]}
													select.select(name="quality" style="margin-left: 20px; margin-right: 20px;" class="qualitySelect")
														each val in VIDEO_QUALITY_LIST
															option(value=""+val[0] selected=(link.quality==val[0])) #{val[1]}
													i.fa(class="fa-times deleteLinkBtn" title="Usuń")
													i.fa(class="fa-check-square-o acceptLinkeBtn" title="Akceptuj")
													.clear

													if link.report!=''
														.zgloszenie Zgłoszenie:
														.report #{REPORT_TYPES_LIST[link.report]}
														.report_desc #{link.report_desc}
														.clear

						if commentsCount>0
							ul#comments_list(data-name="comments" class="par" style=(firstName=='comments'?'display:block':'display:none'))
								each epi in episode_comments
									li.line
										a(href='/serial/'+epi.series_id.url+'/s'+pad(epi.season_num)+'e'+pad(epi.episode_num)+(epi.episode_num_alter?'-e'+pad(epi.episode_num_alter):'')+'/'+epi.url+'/'+epi.num target="_blank")
											img.poster(src=IMG_HOSTNAME+epi.series_id.poster width="24" height="24")
										.titles_box
											.episode s#{pad(epi.season_num)}e#{pad(epi.episode_num)}#{epi.episode_num_alter?'-e'+pad(epi.episode_num_alter):''} !{epi.title}
											.title !{epi.series_id.title}
											.title_org !{epi.series_id.title_org}
										.clear
										ul
											each comment in epi.comments
												li.lineInner(data-ref=""+comment._id)
													a.user(href="/user/"+comment.user.username target="_blank" class="greenLink" title="Dodał:") !{comment.user.username}
													.message #{comment.message}
													i.fa(class="fa-times deleteCommentkBtn" title="Usuń")
													i.fa(class="fa-flag flagCommentkBtn" title="Komentarz zawiera spoilery")
													i.fa(class="fa-check-square-o acceptCommentBtn" title="Akceptuj" style="padding-right: 0px;")
													.clear
								each movie in movie_comments
									li.line
										a(href="/film/"+movie.url+"/"+movie.num target="_blank")
											img.poster(src=IMG_HOSTNAME+movie.poster width="24" height="24")
										.titles_box
											.title !{movie.title}
											.title_org !{movie.title_org}
										.clear
										ul
											each comment in movie.comments
												li.lineInner(data-ref=""+comment._id)
													a.user(href="/user/"+comment.user.username target="_blank" class="greenLink" title="Dodał:") !{comment.user.username}
													.message #{comment.message}
													i.fa(class="fa-times deleteCommentkBtn" title="Usuń")
													i.fa(class="fa-flag flagCommentkBtn" title="Komentarz zawiera spoilery")
													i.fa(class="fa-check-square-o acceptCommentBtn" title="Akceptuj" style="padding-right: 0px;")
													.clear
								each video in video_comments
									li.line
										a(href="/filmik/"+video.url+"/"+video.num target="_blank")
											img.poster(src=IMG_HOSTNAME+video.poster width="50" height="28")
										.titles_box
											.title !{video.title}
										.clear
										ul
											each comment in video.comments
												li.lineInner(data-ref=""+comment._id)
													a.user(href="/user/"+comment.user.username target="_blank" class="greenLink" title="Dodał:") !{comment.user.username}
