											img.poster(src=IMG_HOSTNAME+video.poster width="100" height="56")
										.y_g_box
											.title(style="padding: 15px 0;") !{video.title}
											- var genres = '';
											- for (var i = 0, len = video.genres.length; i < len; i++) {
											-	var uppered = video.genres[i].charAt(0).toUpperCase() + video.genres[i].slice(1);
											- 	if(i==0) genres+= uppered;
											- 	else genres+= ', '+uppered;
											- }
											.genres2 #{genres}
