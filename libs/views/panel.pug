extend main
block content
	#panel_verify_page.box
		.container
			h2.title Panel 
					span.green weryfikacyjny

			#activity_box
				if movies.length == 0 && series.length == 0 && episodes.length == 0 && linksCount == 0 &&  commentsCount == 0 && videos.length == 0
					.d Wszystko sprawdzone.
				else
					#btns
						- var firstName = '';
						if movies.length>0
							- firstName = 'movies';
						if series.length>0 && firstName==''
							- firstName = 'series';
						if episodes.length>0 && firstName==''
							- firstName = 'episodes';
						if linksCount>0 && firstName==''
							- firstName = 'links';
						if commentsCount>0 && firstName==''
							- firstName = 'comments';
						if videos.length>0 && firstName==''
							- firstName = 'videos';

						


						if movies.length>0
							.l_btn(class=(firstName=='movies'?'l_active':'') data-name="movies") Filmy (#{movies.length})
						if series.length>0
							.l_btn(class=(firstName=='series'?'l_active':'') data-name="series") Seriale (#{series.length})
						if episodes.length>0
							.l_btn(class=(firstName=='episodes'?'l_active':'') data-name="episodes") Odcinki (#{episodes.length})
						if linksCount>0
							.l_btn(class=(firstName=='links'?'l_active':'') data-name="links") Linki (#{linksCount})
						if commentsCount>0
							.l_btn(class=(firstName=='comments'?'l_active':'') data-name="comments") Komentarze (#{commentsCount})
						if videos.length>0
							.l_btn(class=(firstName=='videos'?'l_active':'') data-name="videos") Filmiki (#{videos.length})
						if linksCount>0
							.l_btn(data-name="tools") Narzędzia
						.clear
					#contents
						if movies.length>0
							ul#movies_list(data-name="movies" class="par" style=(firstName=='movies'?'display:block':'display:none'))
								if movies.length==0
									li.no_data Brak wyników
								else
									each movie in movies
										li.line(data-ref=""+movie._id)
											a(href=BASE_URL+'/film/'+movie.url+'/'+movie.num target="_blank")
												img.poster(src=IMG_HOSTNAME+movie.poster width="48" height="48")
											.titles_box
												.title !{movie.title}
												.title_org !{movie.title_org}
											.y_g_box
												- var genres = '';
												- for (var i = 0, len = movie.genres.length; i < len; i++) {
												-	var uppered = movie.genres[i].charAt(0).toUpperCase() + movie.genres[i].slice(1);
												- 	if(i==0) genres+= uppered;
												- 	else genres+= ', '+uppered;
												- }
												.genres #{genres}
												.year #{movie.year}
													a.user(href="/user/"+movie.user.username target="_blank" class="greenLink" title="Dodał:") !{movie.user.username}
													span.rate #{Math.round(movie.rate * 100)/100}
											.btn_box
												i.fa(class="fa-times deleteMovieBtn" title="Usuń")
												if movie.filmweb_id!=-1
													a(href="http://www.filmweb.pl/film/"+movie.url+'-'+movie.filmweb_id target="_blank")
														i.fa(class="fa-external-link" title="Otwórz film w filmweb.pl")
												//-i.fa(class="fa-edit" title="Edytuj")
												i.fa(class="fa-check-square-o acceptMovieBtn" title="Akceptuj")
											.clear

											.desc #{movie.desc}

						if series.length>0
