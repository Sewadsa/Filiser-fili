									li.no_data Brak wyników
								else
									each ser in series
										li.line(data-ref=""+ser._id)
											a(href=BASE_URL+'/serial/'+ser.url+'/'+ser.num target="_blank")
												img.poster(src=IMG_HOSTNAME+ser.poster width="48" height="48")
											.titles_box
												.title !{ser.title}
												.title_org !{ser.title_org}
											.y_g_box
												- var genres = '';
												- for (var i = 0, len = ser.genres.length; i < len; i++) {
												-	var uppered = ser.genres[i].charAt(0).toUpperCase() + ser.genres[i].slice(1);
												- 	if(i==0) genres+= uppered;
												- 	else genres+= ', '+uppered;
												- }
												.genres #{genres}
												.year #{ser.year}
													a.user(href="/user/"+ser.user.username target="_blank" class="greenLink" title="Dodał:") !{ser.user.username}
