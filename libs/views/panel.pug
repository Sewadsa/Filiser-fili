													i.fa(class="fa-external-link" title="Otwórz serial w filmweb.pl")
												i.fa(class="fa-edit" title="Edytuj")
												i.fa(class="fa-check-square-o acceptSeriesBtn" title="Akceptuj")
											.clear

											.desc #{ser.desc}
						if episodes.length>0
							ul#episodes_list(data-name="episodes" class="par" style=(firstName=='episodes'?'display:block':'display:none'))
