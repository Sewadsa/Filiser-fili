		if aggregateRatingRate && aggregateRatingRateCount
			div(itemscope itemtype="http://schema.org/Movie" style="display: none;")
				span(itemprop="name") #{aggregateRatingName}
				img(src=aggregateRatingPoster itemprop="image")

				div(itemprop="aggregateRating" itemscope itemtype="http://schema.org/AggregateRating")
					span(itemprop="ratingValue") #{aggregateRatingRate}
					span(itemprop="ratingCount") #{aggregateRatingRateCount}

	body.preload
		if is_mobile
			header#header 
				.container
					a#logo(href="/" tabindex="-1")
						img(src=IMG_HOSTNAME+"/assets/img/logo2.png" width="52" height="32" alt="Logo")

					#hamburger-menu
						i(class="fa fa-bars" aria-hidden="true")
					.clear

					#menu-bg

					#menu
						.logo
							img(src=IMG_HOSTNAME+"/assets/img/logo2.png" width="52" height="32" alt="Logo")

						a(href="/")
							span
								i(class="fa fa-home" aria-hidden="true")
							h2 Home
						a(href="/filmy")
							span
								i(class="fa fa-list" aria-hidden="true")
							h2 Filmy
						a(href="/seriale")
							span
								i(class="fa fa-list" aria-hidden="true")
							h2 Seriale
						a(href="https://filmiki.tv/?utm_source=fili&utm_medium=web&utm_campaign=mega" rel="nofollow noopener noreferrer"  target="_blank")
							span
								i(class="fa fa-list" aria-hidden="true")
							h2 Filmiki

						if user_status.code==3 && (user_status.data.fav_series_count>0 || user_status.data.fav_movies_count>0)
							a(href="/ulubione")
								span
									i(class="fa fa-heart" aria-hidden="true")
								h2 Ulubione
						if user_status.code==3 && (user_status.data.type == 'ADMIN' || user_status.data.type == 'MODERATOR')
							a(href="/panel/weryfikacja")
								span
									i(class="fa fa-cog" aria-hidden="true")
								h2 Panel
						if user_status.code!=3
							a(href="#" class="loginBtn")
								span
									i(class="fa fa-sign-in" aria-hidden="true")
								h2 Zaloguj się
							a(href="#" class="registerBtn")
								span
									i(class="fa fa-user-plus" aria-hidden="true")
								h2 Utwórz konto


						if user_status.code==3
							if PREMIUM
								a(href="/premium")
									span
										i(class="fa fa-star" aria-hidden="true")
									h2 #{user_status.data.premium?'Doładuj premium':'Kup premium'}
							a(href="/user/"+user_status.data.username)
								span
									i(class="fa fa-cog" aria-hidden="true")
								h2 Profil
							a(href="/wyloguj")
								span
									i(class="fa fa-sign-out" aria-hidden="true")
								h2 Wyloguj


			#searchbox
				form(id="searchForm2" style="display: flex;" action="/szukaj" method="GET")
					input.input(type="text" name="q" id="searchbox_in2" placeholder="Szukaj...")
					input.bt(type="submit" value="")


			if user_status.code==3
				#userLine
					.container
						if user_status.data.premium
							span.span Witaj, #{user_status.data.username} !{'~ <span class="premiumColor">Premium kończy się '+MOMENT().to(user_status.data.premium_expire)+'.</span>'}
						else
							span.span Witaj, #{user_status.data.username} ~ <span class="premiumColor">Brak dostępu premium.</span>
		else
			header#header 
				.container
					a#logo(href="/" tabindex="-1")
						img(src=IMG_HOSTNAME+"/assets/img/logo2.png" width="58" height="36" alt="Logo")

					ul#menu
