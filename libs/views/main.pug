						li
							a(href="https://filmiki.tv/?utm_source=fili&utm_medium=web&utm_campaign=mega" title="Filmiki" tabindex="-1" rel="nofollow noopener noreferrer" target="_blank")
								h2 Filmiki

						if user_status.code==3 && (user_status.data.fav_series_count>0 || user_status.data.fav_movies_count>0)
							li
								a(href="/ulubione" title="Ulubione" tabindex="-1")
									h2 Ulubione
						if user_status.code==3 && (user_status.data.type == 'ADMIN' || user_status.data.type == 'MODERATOR')
							li
								a(href="/panel/weryfikacja" title="Panel" tabindex="-1")
									h2 Panel


					#searchbox
						form(id="searchForm")
							input.input(type="text" name="q" id="searchbox_in" placeholder="Szukaj..." tabindex="1")
							input.bt(type="submit" value="" tabindex="-1")
						.results_box
							.loader
								span(class="cssload-loader" style="margin-top: 10px")
									span(class="cssload-loader-inner")
							.no_result
								.t Brak wyników
							.results

					if user_status.code!=3
						#reghbox
							h2.loginBtn Zaloguj 
							.spn /
							h2.registerBtn Utwórz konto
							.clear
					.clear

			if user_status.code==3
				#userLine
					.container
						.left
							if PREMIUM
								span.span Witaj, #{user_status.data.username} !{user_status.data.premium?'~ <span class="premiumColor" title="'+MOMENT(user_status.data.premium_expire).format('DD.MM.YYYY HH:mm:ss')+'">Premium kończy się '+MOMENT().to(user_status.data.premium_expire)+'.</span>':''}
							else
								span.span Witaj, #{user_status.data.username}
						.right
							if PREMIUM
								h2
									a(href="/premium" class="premiumColor") #{user_status.data.premium?'Doładuj premium':'Kup premium'}
							h2
								a(href="/user/"+user_status.data.username) Profil
							h2.addMovieSeriesBtn Dodaj film lub serial
							//- h2
							//- 	a(href="/dodaj-filmik") Dodaj filmik
							h2
								a(href="/wyloguj") Wyloguj
						.clear
		
